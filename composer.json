{
  "name": "coresys/reverse-discriminator",
  "type": "bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "2.9.x-dev",
    "doctrine/doctrine-migrations-bundle": "3.3.x-dev",
    "doctrine/orm": "2.15.x-dev",
    "sensio/framework-extra-bundle": "^6.2",
    "symfony/console": "6.2.*",
    "symfony/dotenv": "6.2.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.2.*",
    "symfony/security-bundle": "6.2.*",
    "symfony/yaml": "6.2.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "CoreSys\\ReverseDiscriminator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoreSys\\ReverseDiscriminator\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.2.*"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
